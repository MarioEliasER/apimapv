@page
@model UbicaTec.Pages.Admin.EliminarModel
@{
}
<!DOCTYPE html>
<html lang="es">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="~/Css/StylesAdmin.css" />
	<link rel="icon" href="~/Images/Icono/Icono144.png" type="image/x-icon">
	<title>Eliminar	</title>
</head>
<body class="grid-container">
	<header class="header">
		<div class="iconTitle">
			<a><img class="icon" src="~/Images/Diseños/Logo.png" alt="icon" /></a>
			<h1>Ubica-Tec</h1>
		</div>
		<a id="Salir"><img class="iconMenu" src="~/Images/Diseños/CerrarSesion.png" alt="filter icon" /></a>
	</header>

	<main class="main">
		<div class="Lista-Container4">
			<p class="MensajeEliminar">Eliminar:</p>
			<div class="Eliminar">
				<label>Edificio Ing. Industrial</label>
				<img id="imagen" class="imagenEliminar" src="~/Images/Diseños/Industrial.png" alt="Imagen predefinida" />
			</div>


			<div class="button-group2">
				<a id="Aceptar" class="btn aceptar"><img src="~/Images/Diseños/Aceptar.png" alt="Aceptar Icono" /> </a>
				<a id="Cancelar" class="btn cancelar"><img src="~/Images/Diseños/Cancelar.png" alt="Cancelar Icono" /> </a>
			</div>

		</div>

	</main>

	<nav class="footer">
		<div class="nav-item">
			<a id="Inicio">
				<img src="~/Images/Diseños/casa.png" alt="home icon" />
				<h3>Inicio</h3>
			</a>
		</div>
	</nav>
	<script src="~/Scripts/Eliminar.js"></script>
	<script>
		//NAVEGACION
		let Home = document.getElementById('Inicio').addEventListener("click", function () {
			window.location.replace("Index");
		});

		let Salir= document.getElementById('Salir').addEventListener("click",	function(){
			window.location.replace("/Index");
		});
		let Aceptar=document.getElementById('Aceptar').addEventListener("click", function(){
			window.location.replace("Index");
		});
		let Cancelar=document.getElementById('Cancelar').addEventListener("click",function(){
			window.location.replace("Index");
		});
	</script>
	<script src="https://kit.fontawesome.com/81581fb069.js" crossorigin="anonymous"></script>
</body>
</html>
